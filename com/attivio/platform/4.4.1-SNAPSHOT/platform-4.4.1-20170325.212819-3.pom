        </dependencies>
              <executions>
                <execution>
                  <id>ROOT POM: dist profile: Do Not Ship Message ${baseName}</id>
                  <phase>install</phase>
                  <goals>
                    <goal>run</goal>
                  </goals>
                  <configuration>
                    <target if="excludedFromAIEProductDistribution">  <!-- skip on if property defined-->
                      <taskdef resource="ise/antelope/tasks/antlib.xml"
                               classpathref="maven.plugin.classpath"/>
                      <taskdef resource="net/sf/antcontrib/antlib.xml"/>
                      <mkdir dir="${distTop}"/>
                      <echo file="${distTop}/Do-not-Distribution-Module-To-Customer-Without-Triage-Team-Authorization.txt">${baseName} is not part of the shipping project.  Please check with triage team prior to distributing to any customer.

                        Module Excluded from QA Validation and Blackduck Intellectual Property Scan.
                        Triage Team:  Jason, Nausheen, Mark S.

                        
                      </echo>
                    </target>
                  </configuration>
                </execution>                                
                <execution>
                  <id>ROOT POM: dist profile: Initial kit files ${baseName}</id>
                  <phase>prepare-package</phase>
                  <goals>
                    <goal>run</goal>
                  </goals>
                  <configuration>
                    <target unless="distInitialKitSkip">  <!-- kit skip used by 3rd party -->
                      <taskdef resource="ise/antelope/tasks/antlib.xml"
                               classpathref="maven.plugin.classpath"/>
                      <taskdef resource="net/sf/antcontrib/antlib.xml"/>

                      <if>
                        <equals arg1="${project.packaging}" arg2="pom" />
                        <then>
                          <property name="skip.pompom.antrun" value="true"/>
                        </then>
                      </if>
                      <property name="skip.pompom.antrun" value="false"/>

                      <if>
                        <equals arg1="${skip.pompom.antrun}" arg2="true" />
                        <then>
                          <echo>ROOT POM: antrun for ${project.packaging} type skipped</echo>
                        </then>
                        <else>

                          <!-- The few lines here are to workaround a javadoc plugin
                          bug. The temporary argfile and options files that get
                          created under target/javadocs/... do not get deleted until
                          after the entire maven build process terminates. We need
                          to manually delete them so the installer does not pick
                          them up. -->
                          <delete quiet="false" failonerror="false">
                            <fileset dir="${project.build.directory}/javadocs" includes="**/options,**/argfile"/>
                          </delete>

                          <!-- I am using antcopies instead of kit assembly because
                          it's much faster. The maven "dir" assembly is painfully
                          slow and getting the maven phase order is difficult. I'm
                          also using exec ant to fork another VM process so ant copy
                          can handle large file copies. Some large files that get
                          copied later on in the reactor barf and fail with
                          "Map failed" errors. This looks like a memory issue with
                          x86 Windows builds and this works around it. -->

                          <if>
                            <equals arg1="${doKit}" arg2="false" />
                            <then>
                              <echo>Skipping kit - doKit property set to false</echo>
                            </then>
                            <else>
                              <exec executable="${antCommand}" dir="${project.basedir}" failonerror="true">
                                <arg value="-DantLoggingEnabled=true"/>
                                <arg value="-Dbasedir=${project.basedir}"/>
                                <arg value="-f"/>
                                <arg value="${project.build.directory}/assemblies/maven-dist.xml"/>
                                <arg value="mavenDist"/>
                                
                              </exec>
                            </else>
                          </if>
                        </else>
                      </if>
                    </target>
                  </configuration>
                </execution>
                <execution>
                  <id>ROOT POM: dist profile antrun: post-dist ${baseName}</id>
                  <phase>install</phase>
                  <goals>
                    <goal>run</goal>
                  </goals>
                  <configuration>
                    <target>
                      <taskdef resource="ise/antelope/tasks/antlib.xml"
                               classpathref="maven.plugin.classpath"/>
                      <taskdef resource="net/sf/antcontrib/antlib.xml"/>

                      <if>
                        <equals arg1="${project.packaging}" arg2="pom" />
                        <then>
                          <property name="skip.pompom.antrun" value="true"/>
                        </then>
                      </if>
                      <property name="skip.pompom.antrun" value="false"/>

                      <if>
                        <equals arg1="${skip.pompom.antrun}" arg2="true" />
                        <then>
                          <echo>ROOT POM: antrun for ${project.packaging} type skipped</echo>
                        </then>
                        <else>
                          <if>
                            <available file="${distTop}"/>
                            <then>
                                  <echo>Generating SHA1 checksums for ${project.artifactId} distribution</echo>
                                  <for param="file">
                                    <path>
                                      <fileset dir="${distTop}">
                                        <exclude name="*docs*" />
                                        <exclude name="*aieDocs*" />
                                        <exclude name="*sha1" />                                        
                                        <include name="*sh" />
                                        <include name="*gz" />
                                        <include name="*zip" />
                                      </fileset>
                                    </path>
                                    <sequential>
                                      <unset name="distroFile"/>
                                      <basename property="distroFile" file="@{file}"/>
                                  <checksum file="${distTop}/${distroFile}" algorithm="SHA1" fileext=".sha1" forceOverwrite="yes"/>
                                    </sequential>
                                  </for>
                            </then>
                            <else>
                              <echo>No post distribution processing required</echo>
                            </else>
                          </if>
                        </else>
                      </if>                      
                    </target>
                  </configuration>
                </execution>            
              </executions>
            </plugin>


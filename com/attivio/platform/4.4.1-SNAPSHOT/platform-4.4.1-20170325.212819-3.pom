           <execution>
             <id>ROOT POM: Kill running surefire-processes environment</id>
             <phase>validate</phase>
             <goals>
               <goal>run</goal>
             </goals>
             <configuration>
               <target>
                 <taskdef resource="ise/antelope/tasks/antlib.xml"
                          classpathref="maven.plugin.classpath"/>
                 <taskdef resource="net/sf/antcontrib/antlib.xml"/>

                 <if>
                   <equals arg1="${project.packaging}" arg2="pom" />
                   <then>
                     <property name="skip.pompom.antrun" value="true"/>
                   </then>
                 </if>
                 <property name="skip.pompom.antrun" value="false"/>

                 <if>
                    <or>
                        <equals arg1="${skipKillingTestProcessesAtModuleStart}" arg2="true" />
                        <equals arg1="${skip.pompom.antrun}" arg2="true" />
                    </or>
                   <then>
                     <echo>ROOT POM: antrun Process Kill for ${project.packaging} type skipped</echo>
                   </then>
                   <else>
                     <mkdir dir="${attivio.log.directory}"/>
                     <exec executable="${java.home}/../bin/jps" failonerror="no" failifexecutionfails="no" output="${javaOnModuleStartLog}">
                        <arg line="-lm"/>
                     </exec>
                     <loadfile srcfile="${javaOnModuleStartLog}" property="fulltext.out"/>
                     <loadfile srcfile="${javaOnModuleStartLog}" property="pids.out">
                        <filterchain>
                            <linecontainsregexp>
                                <regexp pattern="surefire|AieLauncher|OaServerCommandLine"/>
                            </linecontainsregexp>
                            <tokenfilter>
                                <replaceregex pattern="^([0-9]+) .*" replace="\1 "/>
                            </tokenfilter>
                            <ignoreblank/>
                            <striplinebreaks/>
                        </filterchain>
                    </loadfile>
                    <if>
                        <os family="windows"/>
                        <then>
                            <property name="kill.exe" location="${topDir}/tools/cygwin/kill.exe"/>
                            <property name="extraKillArgs" value="-f -9"/>
                        </then>
                        <else>
                            <property name="kill.exe" value="kill"/>
                            <property name="extraKillArgs" value="-9"/>
                        </else>
                    </if>    
                    <if>
                        <isset property="pids.out"/>
                        <then>
                            <echo>Attempted of Test Process : ${kill.exe} ${extraKillArgs} ${pids.out}</echo>
                            <exec executable="${kill.exe}">
                                <arg line="${extraKillArgs} ${pids.out}"/>
                            </exec>
                        </then>
                    </if>
                   </else>
                 </if>
               </target>
             </configuration>
           </execution>

           <execution>
             <id>ROOT POM: Print environment</id>
             <phase>process-resources</phase>
             <goals>
               <goal>run</goal>
             </goals>
             <configuration>
               <target>
                 <taskdef resource="ise/antelope/tasks/antlib.xml"
                          classpathref="maven.plugin.classpath"/>
                 <taskdef resource="net/sf/antcontrib/antlib.xml"/>

                 <if>
                   <equals arg1="${project.packaging}" arg2="pom" />
                   <then>
                     <property name="skip.pompom.antrun" value="true"/>
                   </then>
                 </if>
                 <property name="skip.pompom.antrun" value="false"/>

                 <if>
                   <equals arg1="${skip.pompom.antrun}" arg2="true" />
                   <then>
                     <echo>ROOT POM: antrun for ${project.packaging} type skipped</echo>
                   </then>
                   <else>

                     <!-- Must use attivio.baseport=val. Fail if baseport=val -->
                     <if>
                       <isset property="baseport" />
                       <then>
                         <fail message="[ATTIVIO ERROR] You must use -Dattivio.baseport to define an altername baseport. -Dbaseport is not supported" />
                       </then>
                     </if>

                    <tstamp>
                      <format property="build.datetime" pattern="MM-dd-yyyy_HH:mm:ss" />
                    </tstamp>

                     <grep in="${localRepository}" regex="(url:) (.*)" group="2" property="m2cache" />
                     
                     <echo message="Hostname: ${myhostname}" />
                     <echo message="Time: ${build.datetime}" />
                     <echo message="OS Profile/OS Family/Arch/OS Full: ${osProfile}/${os.family}/${xarchbits}/${os.full}" />
                     <echo message="Baseport: ${attivio.baseport}" />
                     
                     <!-- see java.lang.System or Maven Reference (mvnref-pdf.pdf) "9.2.4 Java System Properties" -->
                     <echo message="Java SRC    ver. -> ${javac.sourceVersion}"/>
                     <echo message="Java Target ver. -> ${javac.targetVersion}"/>
                     <echo message="Java JRE    ver. -> ${java.version}"/>
                     <echo message="Java VM     ver. -> ${java.vm.version}"/>
                     <echo message="Java Home        -> ${java.home}"/>
                     <echo message="Attivio version: ${attivio.version}" />
                     <echo message="Git Branch:         ${scm.branch}" />
                     <echo message="Git ChangsetId:     ${scm.last.change.id}" />
                     <echo message="Last Developer:     ${scm.last.change.user}" />
                     <echo message="Build Number:       ${buildNumber}" />
                     <echo message="MAVEN_OPTS: ${env.MAVEN_OPTS}"/>
                     <echo message="JAVA_OPTS: ${javaMemoryOpts}"/>
                     <echo message="M2 Dir: ${m2cache}"/>
                     <echo message="user.home: ${user.home}"/>
                     <echo message="DISPLAY: ${env.DISPLAY}"/>
                     <echo message="ExeDir:  ${session.executionRootDirectory}"/>
                     <echo message="ProjDir: ${project.basedir}"/>

                     <echo message="Running Java Processes"/>
                     <echo message="----------------------"/>
                     <exec executable="${java.home}/../bin/jps" failonerror="no" failifexecutionfails="no">
                        <arg line="-lm"/>
                     </exec>
                     <echo message="----------------------"/>
                   </else>
                 </if>
               </target>
             </configuration>
           </execution>
           <execution>
             <id>ROOT POM: unpack antbin for ${project.artifactId}</id>
             <phase>process-resources</phase>
             <goals>
               <goal>run</goal>
             </goals>
             <configuration>
               <target>
                 <taskdef resource="ise/antelope/tasks/antlib.xml" classpathref="maven.plugin.classpath"/>
                 <taskdef resource="net/sf/antcontrib/antlib.xml"/>

                 <available file="src/attivio.module.xml" property="hasModuleXml" />
                 <available file="${topDir}/tools/antbin/apache-ant-${org.apache.ant.version}" property="extractedAnt" />

                 <if>
                   <not>
                     <isset property="extractedAnt"/>
                   </not>
                   <then>
                     <echo>Extract ant ${org.apache.ant.version}</echo>
                     <unzip src="${env.M2_REPO}/com/attivio/tools/antbin/ant/${org.apache.ant.version}/ant-${org.apache.ant.version}.zip" dest="${topDir}/tools/antbin">
                    </unzip>
                     <chmod perm="ugo+x">
                       <fileset dir="${topDir}/tools/antbin/apache-ant-${org.apache.ant.version}/bin" />
                     </chmod>
                   </then>
                 </if>
               </target>
             </configuration>
           </execution>
           <execution>
             <id>ROOT POM: Process attivio.module.xml</id>
             <phase>process-resources</phase>
             <goals>
               <goal>run</goal>
             </goals>
             <configuration>
               <target>
                 <taskdef resource="ise/antelope/tasks/antlib.xml" classpathref="maven.plugin.classpath"/>
                 <taskdef resource="net/sf/antcontrib/antlib.xml"/>

                 <available file="src/attivio.module.xml" property="hasModuleXml" />
                 <available file="${project.build.directory}/classes/attivio.module.xml" property="hasGeneratedModuleXml" />

                 <if>
                   <equals arg1="${project.packaging}" arg2="pom" />
                   <then>
                     <property name="skip.pompom.antrun" value="true"/>
                   </then>
                 </if>
                 <property name="skip.pompom.antrun" value="false"/>

                 <if>
                   <equals arg1="${skip.pompom.antrun}" arg2="true" />
                   <then>
                     <echo>ROOT POM: antrun for ${project.packaging} type skipped</echo>
                   </then>
                   <else>
                     <if>
                       <not>
                         <isset property="hasGeneratedModuleXml"/>
                       </not>
                       <then>
                         <if>
                           <isset property="hasModuleXml"/>
                           <then>
                             <copy file="src/attivio.module.xml" todir="${project.build.directory}/classes"/>
                           </then>
                           <else>
                             <echo message="building default module xml" />
                             <echo file="${project.build.directory}/classes/attivio.module.xml" append="false">&lt;attivio&gt;&lt;module name="${baseName}" version="-1" /&gt;&lt;/attivio&gt;</echo>
                           </else>
                         </if>
                       </then>
                     </if>
                     <if>
                       <isset property="excludeAttivioModelXml"/>
                       <then>
                         <delete file="${project.build.directory}/classes/attivio.module.xml" />
                       </then>
                       <else>
                         <replace file="${project.build.directory}/classes/attivio.module.xml" token="PATCH-@attivio.version@" value="PATCH-${attivio.version}" />
                         <replace file="${project.build.directory}/classes/attivio.module.xml" token="version=&quot;-1&quot;" value="version=&quot;${buildNumber}&quot; aie.version=&quot;${attivio.version}&quot;" />
                       </else>
                     </if>
                  </else>
                </if>
               </target>
             </configuration>
           </execution>
           <execution>
             <id>ROOT POM: Generate test-ignore report</id>
             <phase>prepare-package</phase>
             <goals>
               <goal>run</goal>
             </goals>
             <configuration>
               <target>
                 <taskdef resource="ise/antelope/tasks/antlib.xml"
                          classpathref="maven.plugin.classpath"/>
                 <taskdef resource="net/sf/antcontrib/antlib.xml"/>

                 <if>
                   <equals arg1="${project.packaging}" arg2="pom" />
                   <then>
                     <property name="skip.pompom.antrun" value="true"/>
                   </then>
                 </if>
                 <property name="skip.pompom.antrun" value="false"/>

                 <if>
                   <equals arg1="${skip.pompom.antrun}" arg2="true" />
                   <then>
                     <echo>ROOT POM: antrun for ${project.packaging} type skipped</echo>
                   </then>
                   <else>
                     <if>
                       <equals arg1="${skipIgnoredTestReports}" arg2="false" />
                       <then>

                         <property name="compile.path.list" refid="maven.compile.classpath"/>
                         <property name="test.path.list" refid="maven.test.classpath"/>

                         <delete failonerror="false" dir="${project.build.directory}/checkIgnores"/>
                         <mkdir dir="${project.build.directory}/checkIgnores"/>

                         <available file="${topDir}/target/checkIgnores/ignoredTests.html" property="toplevelReportExists"/>

                         <if>
                            <not>
                              <isset property="toplevelReportExists"/>
                            </not>
                            <then>
                              <echo message="&lt;html&gt;&lt;head&gt;&lt;title&gt;Attivio Ignored Tests Report&lt;/title&gt;&lt;body&gt;&lt;b&gt;Attivio Ignored Tests Report - (SVN version ${buildNumber})&lt;/b&gt;&lt;br&gt;&lt;table border=&#8220;1&#8220;&gt;&lt;br&gt;" file="${project.build.directory}/checkIgnores/ignoredTests.txt" append="true" />
                            </then>
                         </if>

                         <javac includeantruntime="false" destdir="${project.build.directory}/checkIgnores" target="${javac.targetVersion}" source="${javac.sourceVersion}" excludes="test/data" debug="on" encoding="utf8">
                           <src path="${project.build.testSourceDirectory}" />
                           <classpath>
                             <pathelement path="${compile.path.list}" />
                             <pathelement path="${test.path.list}" />
                           </classpath>
                           <compilerarg value="-XprintProcessorInfo" />
                           <compilerarg value="-processor"/>
                           <compilerarg value="com.attivio.CheckIgnoreAnnotationProcessor"/>
                           <compilerarg value="-Aid=/${project.name}"/>
                           <compilerarg value="-Afile=${project.build.directory}/checkIgnores/ignoredTests.txt"/>
                         </javac>
                         <available file="${project.build.directory}/checkIgnores/ignoredTests.txt" property="ignoredFileExists"/>
                         <if>
                            <isset property="ignoredFileExists"/>
                            <then>
                              <echo>Adding ${project.build.directory}/checkIgnores/ignoredTests.txt to ${topDir}/target/checkIgnores/ignoredTests.html</echo>
                              <concat destfile="${topDir}/target/checkIgnores/ignoredTests.html" append="true">
                                <filelist dir="${project.build.directory}/checkIgnores" files="ignoredTests.txt"/>
                              </concat>
                            </then>
                            <else>
                              <echo>No ignoredTests found in ${project.build.directory}/checkIgnores/ignoredTests.txt</echo>
                            </else>
                         </if>
                       </then>
                     </if>
                     <delete dir="${project.build.directory}/checkIgnores/com" failonerror="false"/>
                  </else>
                </if>
               </target>
             </configuration>
           </execution>
           <execution>
             <id>ROOT POM: antrun-plugin package phase</id>
             <phase>package</phase>
             <goals>
               <goal>run</goal>
             </goals>
             <configuration>
               <target>
                 <taskdef resource="ise/antelope/tasks/antlib.xml"
                          classpathref="maven.plugin.classpath"/>
                 <taskdef resource="net/sf/antcontrib/antlib.xml"/>

                 <if>
                    <equals arg1="${project.packaging}" arg2="pom" />
                   <then>
                      <property name="generateJavadocs" value="false"/>
                   </then>
                 </if>

                 <if>
                   <equals arg1="${createJavadocs}" arg2="false" />
                   <then>
                      <property name="generateJavadocs" value="false"/>
                   </then>
                 </if>

                 <!-- -Pdist or -Pjavadoc will set javadoc to true -->
                 <if>
                   <equals arg1="${compileJavadoc}" arg2="true" />
                   <then>
                      <property name="generateJavadocs" value="true"/>
                   </then>
                 </if>

                 <property name="generateJavadocs" value="false"/>

                 <echo>INFO: generateJavadocs: ${generateJavadocs}</echo>

                 <!-- I use ant exec to fork another VM to consume less memory
                 and it also builds faster than having all the maven-javadocs.xml
                 logic in this antrun execution block -->
                 <if>
                   <equals arg1="${generateJavadocs}" arg2="true" />
                   <then>

                     <echo message="Check ${baseName} against skip list for javadoc"/>
                     <if>
                       <not>
                         <or>
                            <!-- all part of core-app -->
                           <contains string="${baseName}" substring="core-api" />
                           <contains string="${baseName}" substring="core-kernel" />
                           <contains string="${baseName}" substring="platform-perfmon-server"/>
                           <contains string="${baseName}" substring="platform-perfmon-model"/>
                           <contains string="${baseName}" substring="platform-perfmon-client"/>
                           <contains string="${baseName}" substring="platform-base-connector"/>
                           <contains string="${baseName}" substring="platform-base-transformer"/>
                           <contains string="${baseName}" substring="platform-base-tokenizer"/>
                           <contains string="${baseName}" substring="platform-util"/>
                           <contains string="${baseName}" substring="platform-esb-model"/>
                           <contains string="${baseName}" substring="platform-esb-zoo"/>
                           <contains string="${baseName}" substring="platform-esb-api"/>
                           <contains string="${baseName}" substring="platform-esb-launcher"/>
                           <contains string="${baseName}" substring="platform-esb-exposedapi"/>
                           <contains string="${baseName}" substring="platform-esb-mule"/>
                           <contains string="${baseName}" substring="basetypes"/>
                           <contains string="${baseName}" substring="coretypes"/>
                         </or>
                       </not>
                       <then>
                         <echo>ROOT POM: Generate Attivio Javadocs for ${project.artifactId}</echo>
                         <ant 
                            antfile="${project.build.directory}/assemblies/maven-javadocs.xml"
                            dir="${project.basedir}"
                            target="mavenJavadocs">
                           <property name="basedir" value="${project.basedir}"/>
                           <property name="project.basedir" value="${project.basedir}"/>
                           <property name="aieJavadocTagletsJar" value="${env.M2_REPO}/com/attivio/platform/util/javadoc-taglets/${javadoc-taglets.version}/javadoc-taglets-${javadoc-taglets.version}.jar"/>
                         </ant>

                         <echo>ROOT POM: Copy Attivio Javadocs for ${project.artifactId} to root target/javadocs</echo>
                         <if>
                           <available file="${project.build.directory}/javadocs" />
                           <then>
                               <delete dir="${topDir}/target/javadoc/${baseName}" failonerror="false"/>
                               <echo message="Copying ${project.artifactId} javadoc to ${topDir}/target/javadoc/${baseName}" />
                               <copy todir="${topDir}/target/javadoc" >
                                   <fileset dir="${project.build.directory}/javadocs" includes="**/**" />
                               </copy>
                           </then>
                         </if>
                       </then>
                       <else>
                          <echo>Skipping javadoc for ${project.artifactId} -- included in core-app</echo>
                       </else>                       
                     </if>
                   </then>
                   <else>
                      <echo>Will not generate javadoc for ${project.artifactId}</echo>
                   </else>
                 </if>
               </target>
             </configuration>
           </execution>
         </executions>

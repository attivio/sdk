        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-antrun-plugin</artifactId>
          <version>1.7</version>
          <dependencies>
            <dependency>
              <groupId>com.attivio.platform.tools</groupId>
              <artifactId>aie_checkignores</artifactId>
              <version>${aie_checkignores.version}</version>
            </dependency>
            <dependency>
              <groupId>com.attivio.platform.util</groupId>
              <artifactId>javadoc-taglets</artifactId>
              <version>${javadoc-taglets.version}</version>
            </dependency>
            <dependency>
              <groupId>com.attivio.tools.antbin</groupId>
              <artifactId>ant</artifactId>
              <version>${org.apache.ant.version}</version>
              <type>zip</type>
            </dependency>
            <dependency>
              <groupId>org.apache.ant</groupId>
              <artifactId>ant</artifactId>
              <version>${org.apache.ant.version}</version>
            </dependency>
            <dependency>
              <groupId>ant-contrib</groupId>
              <artifactId>ant-contrib</artifactId>
              <version>1.0b3</version>          
            </dependency>
            <dependency>
              <groupId>ant</groupId>
              <artifactId>ant-optional</artifactId>
              <version>1.5.3-1</version>
            </dependency> 
            <dependency>
              <groupId>org.tigris.antelope</groupId>
              <artifactId>antelopetasks</artifactId>
              <version>3.2.4</version>
            </dependency>
          </dependencies>
          <executions>
            <execution>
            <id>ROOT POM: antrun - Generate build environment properties</id>
            <phase>process-sources</phase>
            <goals>
              <goal>run</goal>
            </goals>
            <configuration>
              <target>
                <taskdef resource="ise/antelope/tasks/antlib.xml" classpathref="maven.plugin.classpath"/>
                <taskdef resource="net/sf/antcontrib/antlib.xml"/>


                <if>
                   <not>
                     <available file="${attivio.git.properties}"/>
                   </not>
                   <then>
                    <echo></echo>
                    <echo>=============== E R R O R ===============</echo>
                    <echo> Root pom must build before building other modules to create ${attivio.git.properties}</echo>
                    <echo>  To fix: </echo>
                    <echo>   cd to root</echo>
                    <echo>   build root pom non recursively:   mvn -N</echo>
                    <echo></echo>
                    <echo>=========================================</echo>
                    <echo></echo>
                    <fail>Cannot Determine Git Information</fail>                    
                   </then>
                </if>
                <property file="${attivio.git.properties}"/>

                <mkdir dir="${project.build.directory}/classes/META-INF"/>
                <mkdir dir="${runtime.report.dir}"/>
                <mkdir dir="${attivio.log.directory}"/>

                <tstamp>
                  <format property="build.date" pattern="yyyyMMdd" />
                  <format property="build.year" pattern="yyyy" />
                  <format property="touch.time" pattern="MM-dd-yyyy_HH:mm:ss_Z" />
                  <format property="build.time" pattern="HHmm" />
                </tstamp>
               
                <!--  Playing around with ppids but Windows is not cooperating
                <exec dir="." executable="${perlExe}" outputproperty="PID"
                  failonerror="true">
                  <arg line="${envToolsDir}/bin/pid.pl" />
                </exec>
                <echo>PID: ${PID}</echo>
                -->

                <!-- Needed for antrun javadoc maven-javadocs.xml  - mavenJavadocs-->
                <property name="javadoc.test.path.list" refid="maven.test.classpath"/>
                <property name="javadoc.compile.path.list" refid="maven.compile.classpath"/>
                <!-- Create a nothing value so we have at least one val -->
                <echo message="dummy.value=nothing${line.separator}" file="${antrun.properties}" />
                <echo message="javadoc.test.path.list=${javadoc.test.path.list}${line.separator}" file="${antrun.properties}" append="true" />
                <echo message="javadoc.compile.path.list=${javadoc.compile.path.list}${line.separator}" file="${antrun.properties}" append="true" />
                <echo message="ldLibraryAddon=${ldLibraryAddon}${line.separator}" file="${antrun.properties}" append="true" />

                <echo>Converting backslashes to forward in antrun.properties</echo>
                <replaceregexp file="${antrun.properties}"
                  match="\\"
                  replace="/"
                  byline="true"
                  flags="g"
                />

                <property name="NL" value="&#13;&#10;"/>
                <property name="version.properties" value="${project.basedir}/target/version.properties"/>

                <echo>Creating Attivio target/version.properties file</echo>

                <propertyregex property="attivio.version"
                  input="${project.version}"
                  regexp="(-SNAPSHOT)?"
                  replace=""
                />




                <echo message="attivio.version=${attivio.version}${line.separator}" file="${version.properties}" />
                <echo message="thisVersion=${attivio.version}${line.separator}" file="${version.properties}" append="true" />
                <echo message="buildNumber=${buildNumber}${line.separator}" file="${version.properties}" append="true" />
                <echo message="scm.branch=${git.branch}${line.separator}" file="${version.properties}" append="true" />
                <echo message="scm.last.change.id=${git.commit.id}${line.separator}" file="${version.properties}" append="true" />
                <echo message="scm.last.change.user=${git.commit.user.name}${line.separator}" file="${version.properties}" append="true" />
                <echo message="build.date=${build.date}${line.separator}" file="${version.properties}" append="true" />
                <echo message="build.year=${build.year}${line.separator}" file="${version.properties}" append="true" />
                <echo message="build.time=${build.time}${line.separator}" file="${version.properties}" append="true" />

                <echo>ROOT POM: Create target/classes/META-INF/MANIFEST.MF</echo>
                <manifest file="${project.build.directory}/classes/META-INF/MANIFEST.MF">
                  <section name="Attivio">
                   <attribute name="Built-By" value="${user.name}" />
                   <attribute name="SCM-Last-Change-Id"   value="${git.commit.id}" /> 
                   <attribute name="SCM-Last-Change-User" value="${git.commit.user.name}" />
                   <attribute name="SCM-Branch" value="${git.branch}"/>
                   <attribute name="Build-Number" value="${buildNumber}" />                   
                   <attribute name="Built-On" value="${build.date}" />
                   <attribute name="Module" value="${baseName}" />
                   <attribute name="Version" value="${attivio.version}"/>
                  </section>
                </manifest>

                <!-- Just in case the main MANIFEST has a Class: ref -->
                <copy file="${project.build.directory}/classes/META-INF/MANIFEST.MF" tofile="${project.build.directory}/main-manifest/META-INF/MANIFEST.MF" overwrite="true"/>

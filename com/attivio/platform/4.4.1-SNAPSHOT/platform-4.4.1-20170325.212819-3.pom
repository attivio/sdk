                </execution>
                <execution>
                  <id>ROOT POM: dist profile: Copy all dependencies for ${baseName}</id>
                  <phase>prepare-package</phase>
                  <goals>
                    <goal>copy-dependencies</goal>
                  </goals>
                  <configuration>
                    <!-- This skip still doesn't work in 2.2 -->
                    <!-- It's only been added for copy and unpack!!!! -->
                    <skip>${skipMavenDependencyPlugin}</skip>
                    <stripVersion>${versionStrip}</stripVersion>
                    <useBaseVersion>true</useBaseVersion>
                    <outputDirectory>${defaultKitlibDir}</outputDirectory>
                    <excludeTransitive>${transitiveExcludes}</excludeTransitive>
                    <excludeTypes>swc</excludeTypes> <!-- No swc flex types -->
                    <overWriteIfNewer>true</overWriteIfNewer>

                    <!-- Basically excludes provided and test scoped deps -->
                    <includeScope>runtime</includeScope>
                    <excludeScope>provided</excludeScope>
                    <excludeGroupIds>${groupIdKitExcludes}</excludeGroupIds>
                    <excludeArtifactIds>${artifactIdKitExcludes}</excludeArtifactIds>
                    <includeArtifactIds>${includeArtifactIds}</includeArtifactIds>
                    <includeGroupIds>${includeGroupIds}</includeGroupIds>
                  </configuration>
                </execution>
              </executions>
            </plugin>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>exec-maven-plugin</artifactId>

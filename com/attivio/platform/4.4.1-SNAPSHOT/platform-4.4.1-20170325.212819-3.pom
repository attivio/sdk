    <profile>
      <id>dist</id>
      <activation>
        <property>
          <name>dist</name>
          <value>true</value>
        </property>
      </activation>
      <properties>
        <skipJavadocPlugin>false</skipJavadocPlugin>
        <compileJavadoc>true</compileJavadoc>
      </properties>
      <build>
        <pluginManagement>
          <plugins>
            <plugin>
              <groupId>org.apache.maven.plugins</groupId>

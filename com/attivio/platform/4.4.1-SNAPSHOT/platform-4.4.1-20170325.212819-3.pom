  <profiles>
    <profile>
      <id>sdkinstall</id>
      <distributionManagement>
	<!-- This serves as the internal repo that is used as the target when building up what the SDK repo
	     is going to look like.  -->
	<repository>
	  <id>internal.repo</id>
	  <name>Temporary Staging Repository</name>
	  <url>file://${top.dir}/target/mvn-repo</url>
	</repository>
      </distributionManagement>

      <build>
	<pluginManagement>
	  <plugins>
	    <plugin>
	      <!-- Don't copy sources when creating the SDK repo -->
	      <groupId>org.apache.maven.plugins</groupId>
	      <artifactId>maven-source-plugin</artifactId>
	      <version>2.2.1</version>
	      <configuration>
		<skipSource>true</skipSource>
	      </configuration>
	    </plugin>
	    <plugin>
	      <!-- When creating the SDK repo, deploy targets the shared SDK repo -->
	      <artifactId>maven-deploy-plugin</artifactId>
	      <version>2.8.1</version>
	      <configuration>
		<altDeploymentRepository>internal.repo::default::file://${top.dir}/target/mvn-repo</altDeploymentRepository>
	      </configuration>
	    </plugin>
	  </plugins>  
	</pluginManagement>
	<plugins>
	  <plugin>
	    <!-- Copy the .md files for the SDK repo (the documentation on how to use it) -->
	    <artifactId>maven-resources-plugin</artifactId>
	    <executions>
	      <execution>
		<id>copy public SDK documentation</id>
		<phase>generate-resources</phase>
		<goals>
		  <goal>copy-resources</goal>
		</goals>
		<configuration>
		  <outputDirectory>${top.dir}/target/mvn-repo</outputDirectory>
		  <overwrite>true</overwrite>
		  <resources>
                    <resource>
                      <directory>src/main/resources</directory>
                    </resource>
		  </resources>
		</configuration>
	      </execution>
	    </executions>
	  </plugin>
	</plugins>
      </build>
    </profile>

    <profile>
      <id>sdkdeploy</id>
      <build>
	<pluginManagement>
	  <plugins>
	    <plugin>
	      <!-- When creating the SDK repo, deploy targets the shared SDK repo -->
	      <artifactId>maven-deploy-plugin</artifactId>
	      <version>2.8.1</version>
	      <configuration>
		<altDeploymentRepository>internal.repo::default::file://${top.dir}/target/mvn-repo</altDeploymentRepository>
	      </configuration>
	    </plugin>
	  </plugins>  
	</pluginManagement>
	<plugins>
	  <plugin>
	    <!-- This plugin takes the shared SDK repo and pushes it as a single commit to the target github host -->
            <groupId>com.github.github</groupId>
            <artifactId>site-maven-plugin</artifactId>
            <version>0.12</version>
            <configuration>
	      <message>Maven artifacts for ${project.version}</message>
	      <noJekyll>true</noJekyll>
	      <outputDirectory>${top.dir}/target/mvn-repo</outputDirectory> 
	      <branch>refs/heads/4.4</branch>
	      <includes>
		<include>**/*</include>
	      </includes>
	      <repositoryName>sdk</repositoryName>      <!-- github repo name -->
	      <repositoryOwner>attivio</repositoryOwner>    <!-- github username  -->
	      <!--<host>https://git.attivio.com/api/v3</host>-->
            </configuration>
            <executions>
	      <!-- run site-maven-plugin's 'site' target as part of the build's normal 'deploy' phase -->
	      <execution>
                <goals>
                  <goal>site</goal>
                </goals>
                <phase>deploy</phase>
	      </execution>
            </executions>
          </plugin>
	</plugins>
      </build>
    </profile>


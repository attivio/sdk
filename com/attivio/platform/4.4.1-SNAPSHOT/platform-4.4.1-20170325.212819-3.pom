        <plugin>
          <groupId>com.atlassian.maven.plugins</groupId>
          <artifactId>maven-clover2-plugin</artifactId>
          <version>${clover.version}</version>
          <configuration>
            <skip>${skipClover}</skip>
            <licenseLocation>${topDir}/tools/clover/clover.license</licenseLocation>
            <includesTestSourceRoots>false</includesTestSourceRoots>
            <includes>
              <include>**/com/attivio/**/*.java</include>
            </includes>
            <excludes>
              <exclude>**/com/attivio/connector/plugin/exchange/ews/ExchangeServiceStub.java</exclude>
              <exclude>**/com/attivio/designer/**/*Activator*.java</exclude>
              <exclude>**/com/attivio/designer/swtbot/**/*.java</exclude>
              <exclude>**/com/attivio/designer/testUtil/**/*.java</exclude>
              <exclude>**/com/attivio/designer/testUtil/**/ResourceHelper.java</exclude>
              <exclude>**/com/attivio/designer/**/*Test*.java</exclude>
              <exclude>**/com/attivio/ontologyui/client/*.java</exclude>
            </excludes>
          </configuration>
        </plugin>

        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-surefire-report-plugin</artifactId>
          <version>${mavenSurefireReportPluginVersion}</version>
          <executions>
            <execution>
              <id>ROOT POM: Generate Surefire Reports</id>
              <phase>test</phase>
              <goals>
                <goal>report-only</goal>
              </goals>
            </execution>
          </executions>
          <configuration>
           <aggregate>${surefireAggregateReport}</aggregate>
          </configuration>
        </plugin>

        <plugin>
          <groupId>org.codehaus.mojo</groupId>
          <artifactId>findbugs-maven-plugin</artifactId>
          <version>${findbugsPluginVersion}</version>
        </plugin>

    <plugin>
      <groupId>org.eclipse.m2e</groupId>
        <artifactId>lifecycle-mapping</artifactId>
        <version>1.0.0</version>
        <configuration>
          <lifecycleMappingMetadata>
            <pluginExecutions>
              <pluginExecution>
                <pluginExecutionFilter>
                  <groupId>org.apache.maven.plugins</groupId>
                  <artifactId>maven-dependency-plugin</artifactId>
                  <versionRange>[1.0.0,)</versionRange>
                  <goals>
                                        <goal>copy</goal>
                    <goal>copy-dependencies</goal>
                    <goal>unpack</goal>
                  </goals>
                </pluginExecutionFilter>
                <action>
                  <ignore />
                </action>
              </pluginExecution>
              <pluginExecution>
                <pluginExecutionFilter>
                  <groupId>org.apache.maven.plugins</groupId>
                  <artifactId>maven-antrun-plugin</artifactId>
                  <versionRange>[1.0.0,)</versionRange>
                  <goals>
                    <goal>run</goal>
                  </goals>
                </pluginExecutionFilter>
                <action>
                  <ignore />
                </action>
              </pluginExecution>
              <pluginExecution>
                <pluginExecutionFilter>
                  <groupId>org.codehaus.mojo</groupId>
                  <artifactId>properties-maven-plugin</artifactId>
                  <versionRange>[1.0-alpha-2,)</versionRange>
                  <goals>
                    <goal>read-project-properties</goal>
                    <goal>write-project-properties</goal>
                  </goals>
                </pluginExecutionFilter>
                <action>
                  <ignore />
                </action>
              </pluginExecution>              
              <pluginExecution>
                <pluginExecutionFilter>
                  <groupId>org.codehaus.mojo</groupId>
                  <artifactId>javacc-maven-plugin</artifactId>
                  <versionRange>[1.0,)</versionRange>
                  <goals>
                    <goal>javacc</goal>
                  </goals>
                                </pluginExecutionFilter>
                <action>
                  <ignore />
                </action>
              </pluginExecution>              
                            <pluginExecution>
                                <pluginExecutionFilter>
                                    <groupId>org.codehaus.mojo</groupId>
                                    <artifactId>native-maven-plugin</artifactId>
                                    <versionRange>[1.0*,)</versionRange>
                                    <goals>
                                        <goal>compile</goal>
                                        <goal>initialize</goal>
                                        <goal>link</goal>
                                        <goal>unzipinc</goal>
                                    </goals>
                                </pluginExecutionFilter>
                                <action>
                                    <ignore />
                                </action>
                            </pluginExecution>              
                            <pluginExecution>
                                <pluginExecutionFilter>
                                    <groupId>org.codehaus.mojo</groupId>
                                    <artifactId>selenium-maven-plugin</artifactId>
                                    <versionRange>[2.0-SNAPSHOT,)</versionRange>
                                    <goals>
                                        <goal>xvfb</goal>
                                    </goals>
                                </pluginExecutionFilter>
                                <action>
                                    <ignore />
                                </action>
                            </pluginExecution>              
            </pluginExecutions>
          </lifecycleMappingMetadata>
        </configuration>
      </plugin>        

          <plugin>
                <!-- Need to copy sources to target/classes for proper GWT js compilation 
                    running from an eclipse project -->
                <artifactId>maven-resources-plugin</artifactId>
                <executions>
                    <execution>
                        <id>copy-java-files-for-gwt-elipse</id>
                        <phase>process-sources</phase>
                        <goals>
                            <goal>copy-resources</goal>
                        </goals>
                        <configuration>
                            <outputDirectory>${project.build.directory}/classes</outputDirectory>
                            <resources>
                                <resource>
                                    <directory>src</directory>
                                    <includes>
                                        <include>**/*.gwt.xml</include>
                                        <include>**/*.java</include>
                                        <include>**/*.png</include>
                                        <include>**/*.gif</include>
                                        <include>**/*.css</include>
                                    </includes>
                  <excludes>
                    <exclude>${gwt.copy.src.exclude}</exclude>
                  </excludes>
                                </resource>
                                <resource>
                                    <directory>super</directory>
                                    <includes>
                                        <include>**/*.gwt.xml</include>
                                        <include>**/*.java</include>
                                        <include>**/*.png</include>
                                        <include>**/*.gif</include>
                                        <include>**/*.css</include>
                                    </includes>
                  <excludes>
                    <exclude>${gwt.copy.src.exclude}</exclude>
                  </excludes>
                                </resource>
                            </resources>
                        </configuration>
                    </execution>
                    <execution>
                        <id>copy-test-files-for-gwt-elipse</id>
                        <phase>process-sources</phase>
                        <goals>
                            <goal>copy-resources</goal>
                        </goals>
                        <configuration>
                            <outputDirectory>${project.build.directory}/test-classes</outputDirectory>
                            <resources>
                                <resource>
                                    <directory>test</directory>
                                    <includes>
                                        <include>**/*.gwt.xml</include>
                                        <include>**/*.java</include>
                                        <include>**/*.png</include>
                                        <include>**/*.gif</include>
                                        <include>**/*.css</include>
                                    </includes>
                  <excludes>
                    <exclude>${gwt.copy.src.exclude}</exclude>
                  </excludes>
                                </resource>
                            </resources>
                        </configuration>
                    </execution>
                    <execution>
                        <id>copy-html-files-for-gwt-elipse</id>
                        <phase>process-sources</phase>
                        <goals>
                            <goal>copy-resources</goal>
                        </goals>
                        <configuration>
                            <outputDirectory>${project.build.directory}/test-webapps</outputDirectory>
                            <resources>
                                <resource>
                                    <directory>webapps</directory>
                                    <includes>
                                        <include>**/*.*</include>
                                    </includes>
                  <excludes>
                    <exclude>${gwt.copy.src.exclude}</exclude>
                  </excludes>
                                </resource>
                            </resources>
                        </configuration>
                    </execution>
                </executions>
          </plugin>


      </plugins>

    </pluginManagement>

    <!-- All plugins below are inherited by all leaf poms -->
    <plugins>
      <plugin>
	<!-- This sets the property top.dir to the absolute path of the top directory.  The property can only
	     be used within other plugin configuration.  -->
        <groupId>org.commonjava.maven.plugins</groupId>
        <artifactId>directory-maven-plugin</artifactId>
        <version>0.1</version>
        <executions>
          <execution>
            <id>directories</id>
            <goals>
              <goal>directory-of</goal>
            </goals>
            <phase>initialize</phase>
            <configuration>
              <property>top.dir</property>
              <project>
                <groupId>com.attivio</groupId>
                <artifactId>platform</artifactId>
              </project>
            </configuration>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <groupId>pl.project13.maven</groupId>
        <artifactId>git-commit-id-plugin</artifactId>
        <version>2.2.0</version>
        <executions>
            <execution>
                <phase>initialize</phase>
                <goals>
                    <goal>revision</goal>
                </goals>
            </execution>
        </executions>
        <configuration>
            <useNativeGit>true</useNativeGit>
            <runOnlyOnce>true</runOnlyOnce>
            <skipPoms>false</skipPoms>
            <verbose>true</verbose>
            <generateGitPropertiesFile>true</generateGitPropertiesFile>
            <generateGitPropertiesFilename>${attivio.git.properties}</generateGitPropertiesFilename>
            <dotGitDirectory>${topDir}/.git</dotGitDirectory>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-deploy-plugin</artifactId>
        <version>2.5</version>
        <configuration>
          <skip>${skipNexusDeploy}</skip>
          <repositoryId>${snapshots}</repositoryId>
          <url>${maven.snapshotsRepository}</url>
        </configuration>
      </plugin>

      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-resources-plugin</artifactId>
        <version>2.4.3</version>
        <configuration>
          <delimiters>
            <delimiter>${*}</delimiter>
            <delimiter>@</delimiter>
          </delimiters>
        </configuration>
      </plugin>

      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-enforcer-plugin</artifactId>
      </plugin>

      <plugin>
        <groupId>org.eclipse.tycho</groupId>
        <artifactId>tycho-surefire-plugin</artifactId>
        <version>${tycho-version}</version>
        <configuration>
            <argLine>-Xbootclasspath/p:${env.M2_REPO}/com/cenqua/clover/clover/3.1.11/clover-3.1.11.jar -Xms1024m -Xmx1536m -XX:MaxPermSize=512m</argLine>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-failsafe-plugin</artifactId>
      </plugin>
    </plugins>

  </build>

</project>
